{
  "name": "oxql-grammar",
  "displayName": "oxql-grammar",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oxidecomputer/oxql-grammar.git"
  },
  "author": "Oxide Computer Company",
  "description": "Syntax highlighting for OxQL (Oximeter Query Language)",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.92.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "oxql",
      "aliases": ["OxQL", "oxql"],
      "extensions": [",oxql"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "oxql",
      "scopeName": "source.oxql",
      "path": "./syntaxes/oxql.tmLanguage.json"
    }]
  }
}
